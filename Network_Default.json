{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Template to launch an Amazon VPC.",
    "Resources": {
      "VPCParis": {
        "Type": "AWS::EC2::VPC",
        "Properties": {
          "EnableDnsSupport": "true",
          "EnableDnsHostnames": "true",
          "CidrBlock" : "10.0.0.0/16",
          "Tags": [
            {
                "Key": "Name",
                "Value": "VPCFrance"
            },
            {
                "Key": "Region",
                "Value": "EU-WEST-3"

            }
          ]
        }
      },
      "PublicSubnet":{
        "Type": "AWS::EC2::Subnet",
        "Properties": {
          "AssignIpv6AddressOnCreation": "false",
          "AvailabilityZone": {
            "Ref": "PublicAvailabilityZone"
          },
          "CidrBlock": "10.0.1.0/24",
          "MapPublicIpOnLaunch" : "true",
          "Tags": [
            {
              "Key": "Name",
              "Value": "PublicSubnet"
            }
          ],
          "VpcId": {
            "Ref": "VPCParis"
          }
        }
      }
    },
    "Parameters": {
        "PublicAvailabilityZone":{
            "Description": "Network AZ",
            "Type": "String",
            "Default": "eu-west-3a",
            "AllowedValues": [
                "eu-west-3a",
                "eu-west-3b",
                "eu-west-3c"
            ]
        }
      }
  }
