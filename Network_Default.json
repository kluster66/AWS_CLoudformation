{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Template to launch an Amazon VPC.",
    "Resources": {
      "VPCParis": {
        "Type": "AWS::EC2::VPC",
        "Properties": {
          "EnableDnsSupport": "true",
          "EnableDnsHostnames": "true",
          "CidrBlock" : "10.0.0.0/16",
          "Tags": [
            {
                "Key": "Name",
                "Value": "VPCFrance"
            },
            {
                "Key": "Region",
                "Value": "EU-WEST-3"

            }
          ]
        }
      },
      "PublicSubnet":{
        "Type": "AWS::EC2::Subnet",
        "Properties": {
          "AssignIpv6AddressOnCreation": "false",
          "AvailabilityZone": String,
          "CidrBlock": "10.0.1.0/24",
          "MapPublicIpOnLaunch" : "true",
          "Tags": [
            {
              "Key": "Name",
              "value": "PublicSubnet"
            }
          ],
          "VpcId": {
            "Ref": "VPCParis"
          }

        }
      },
      "PrivateSubnet":{
        "Type": "AWS::EC2::Subnet",
        "Properties": {

        }
      }
    },
    "Output":
    {}
  }
