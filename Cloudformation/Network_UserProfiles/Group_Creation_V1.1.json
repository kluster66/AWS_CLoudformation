{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template we are creating four IAM groups namely admingroup, devlopergroup ,readonlygroup , powerusergroup which we will use to add respective users (Admin, ReadOnly,Developer and Power user) into the IAM groups",
    "Parameters" : {
     "DevGroupName": {
      "Type": "String",
	  "Default": "DevloperGroup",
      "Description" : "Developer Group Name"
    },
	"AdminGroupName": {
      "Type": "String",
	  "Default": "AdminGroup",
      "Description" : "Admin Group Name"
    },
	"ReadOnlyGroupName": {
      "Type": "String",
	  "Default": "ReadOnlyGroup",
      "Description" : "ReadOnly Group Name"
    },
	"PowerUserGroupName": {
      "Type": "String",
	  "Default": "PowerUserGroup",
      "Description" : "PowerUser Group Name"
    }   	
	},
  "Resources": {
    "DevloperGroup": {
      "Type": "AWS::IAM::Group",
      "Properties": {
        "GroupName": {"Ref" : "DevGroupName"}
      }
    },
    "AdminGroup": {
      "Type": "AWS::IAM::Group",
      "Properties": {
        "GroupName": {"Ref" : "AdminGroupName"}
      }
    },
    "ReadOnlyGroup": {
      "Type": "AWS::IAM::Group",
      "Properties": {
        "GroupName": {"Ref" : "ReadOnlyGroupName"}
      }
    },
    "PowerUserGroup": {
      "Type": "AWS::IAM::Group",
      "Properties": {
        "GroupName": {"Ref" : "PowerUserGroupName"}
      }
    } 
    },
	 "Outputs": {
   
    "DevGroup": {
      "Description": "Developer Group",
      "Value": {
        "Ref": "DevloperGroup"
      },
      "Export": {
        "Name": "DevGroup"
        
      }
    },
	"AdministratorGroup": {
      "Description": "Administrator Group",
      "Value": {
        "Ref": "AdminGroup"
      },
      "Export": {
        "Name": "AdministratorGroup"
        
      }
    },
	"ReadOnlyGroup": {
      "Description": "ReadOnlyGroup Group",
      "Value": {
        "Ref": "ReadOnlyGroup"
      },
      "Export": {
        "Name": "ReadOnlyGroup"
        
      }
    },
	"PowerUserGroup": {
      "Description": "PowerUserGroup Group",
      "Value": {
        "Ref": "PowerUserGroup"
      },
      "Export": {
        "Name": "PowerUserGroup"
        
      }
    }
  }
}
